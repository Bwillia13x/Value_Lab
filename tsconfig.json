{
  "compilerOptions": {
    "target": "ES2019",
    "module": "CommonJS",
    "rootDir": "src",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["jest", "node"],
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "dist"]
}